{
  "name": "@urlpack/base-codec",
  "version": "2.0.0",
  "license": "MIT",
  "homepage": "https://github.com/daangn/urlpack/tree/main/packages/base-codec",
  "repository": {
    "type": "git",
    "url": "https://github.com/daangn/urlpack.git",
    "directory": "packages/base-codec"
  },
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.mjs",
