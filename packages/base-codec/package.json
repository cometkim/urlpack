  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "prepack": "yarn build",
    "build": "nanobundle build --standalone"
  },
  "devDependencies": {
    "nanobundle": "^2.0.0",
    "typescript": "^5.4.3"
  }
