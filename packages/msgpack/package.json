{
  "name": "@urlpack/msgpack",
  "version": "0.0.1",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./lib/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.js",
    "module": "./lib/index.module.js",
    "unpkg": "./lib/index.umd.js",
    "types": "./lib/index.d.ts",
    "exports": {
      ".": {
        "require": "./lib/index.js",
        "import": "./lib/index.module.js"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "test": "uvu -r esbuild-register",
    "test:watch": "yarn test || true && watchlist src tests -- yarn test",
    "build": "microbundle",
    "dev": "microbundle watch",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "esbuild": "^0.12.15",
    "esbuild-register": "^2.6.0",
    "microbundle": "^0.13.3",
    "typescript": "^4.3.5",
    "uvu": "^0.5.1",
    "watchlist": "^0.2.3"
  }
}
