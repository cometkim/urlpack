{
  "name": "@urlpack/json",
  "version": "1.0.4",
  "license": "MIT",
  "sideEffects": false,
  "homepage": "https://github.com/daangn/urlpack/tree/main/packages/json",
  "repository": {
    "type": "git",
    "url": "https://github.com/daangn/urlpack.git",
    "directory": "packages/json"
  },
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.js",
    "module": "./lib/index.module.js",
    "types": "./lib/index.d.ts",
    "exports": {
      ".": {
        "require": "./lib/index.js",
        "import": "./lib/index.module.js"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "test": "uvu -r esbuild-register",
    "test:watch": "watchlist src tests -- yarn test",
    "build": "microbundle --format esm,cjs",
    "dev": "microbundle watch",
    "prepack": "yarn build"
  },
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "@urlpack/base58": "workspace:^1.0.4",
    "@urlpack/msgpack": "workspace:^1.0.0"
  },
  "devDependencies": {
    "esbuild-register": "^2.6.0",
    "microbundle": "^0.13.3",
    "typescript": "^4.3.5",
    "uvu": "^0.5.1",
    "watchlist": "^0.2.3"
  }
}
