{
  "name": "urlpack",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./lib/index.js",
    "module": "./lib/index.module.js",
    "types": "./lib/index.d.ts",
    "exports": {
      ".": {
        "require": "./lib/index.js",
        "import": "./lib/index.module.js"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "test": "uvu -r esbuild-register",
    "test:watch": "watchlist src tests -- yarn test",
    "build": "microbundle --format esm,cjs",
    "dev": "microbundle watch",
    "prepack": "yarn build"
  },
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "@urlpack/base58": "*",
    "@urlpack/msgpack": "*"
  },
  "devDependencies": {
    "esbuild-register": "^2.6.0",
    "microbundle": "^0.13.3",
    "typescript": "^4.3.5",
    "uvu": "^0.5.1",
    "watchlist": "^0.2.3"
  }
}
